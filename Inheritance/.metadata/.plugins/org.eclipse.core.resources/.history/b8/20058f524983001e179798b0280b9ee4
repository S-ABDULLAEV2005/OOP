package my_pack;
import java.util.HashMap;
public class HashMapAwesomeness {

	public static void main(String[] args) {
	String a = "10";
	String b = "55";
	int c = 88;
	System.out.println(a + b + c);
	HashMap<String, Integer> happy = new HashMap<String,Integer>();
	happy.put("a", 10);
	happy.put("b", 3);
	happy.put("c", 88);
	
	System.out.println(happy.get(c));
	}

}
