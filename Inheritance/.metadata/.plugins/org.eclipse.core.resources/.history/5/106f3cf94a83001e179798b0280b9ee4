package my_pack;
import java.util.HashMap;
public class HashMapAwesomeness {

	public static void main(String[] args) {

		HashMap<String, String> fun = new HashMap<String, String>();
		fun.put("Shakha2005", "Leo2005");
		fun.put("Leo2023", "qwerty");
		fun.remove("Shakha2005");
		System.out.println(fun);
		System.out.println(fun.containsKey("Leo2023"));
		System.out.println(fun.replace("Leo2023", "mama"));
		
		
		

		
		
